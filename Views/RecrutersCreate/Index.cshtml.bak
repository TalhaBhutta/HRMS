<script>
    // Example starter JavaScript for disabling form submissions if there are invalid fields
    (function () {
        'use strict';
        window.addEventListener('load', function () {
            // Fetch all the forms we want to apply custom Bootstrap validation styles to
            var forms = document.getElementsByClassName('needs-validation');
            // Loop over them and prevent submission
            var validation = Array.prototype.filter.call(forms, function (form) {
                form.addEventListener('submit', function (event) {
                    if (form.checkValidity() === false) {
                        //alert('Make sure all required fields are filled and try again.');
                        document.getElementById("incompletedFormAlert").style.display = "block";
                        //if (x.style.display === "none") {
                        //    x.style.display = "block";
                        //} else {
                        //    x.style.display = "none";
                        //}
                        document.getElementById("confirmDetails").style.display = "none";
                        document.getElementById("circleSuccess").style.display = "none";
                        //y.style.display = "none";

                        event.preventDefault();
                        event.stopPropagation();
                    } else {
                        document.getElementById("circleSuccess").style.display = "block";
                        document.getElementById("incompletedFormAlert").style.display = "none";
                    }
                    form.classList.add('was-validated');
                }, false);
            });
        }, false);

    })();
</script>

