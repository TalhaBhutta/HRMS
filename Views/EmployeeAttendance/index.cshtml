@using HRMS.Enums
@model HRMS.Models.Attendance

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

    ViewBag.Title = "Employee Attendance";
    ViewBag.pTitle = "Employee Attendance";
    ViewBag.pageTitle = "Attendance";
    Layout = "~/Views/_Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html lang="en">

<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="/assets/style.css" rel="stylesheet" type="text/css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body>
    <div class=" pt-5">
        <div class="row">
            <div class="col-12">
                <div id="table-actions" class="flex-grow-1 align-items-center d-flex py-3">
                    <button class="btn btn-primary rounded f-14 p-2 mr-3 float-left" data-bs-toggle="modal" data-bs-target="#add-attendance">
                        <svg class="svg-inline--fa fa-plus fa-w-14 mr-1" aria-hidden="true" focusable="false"
                             data-prefix="fa" data-icon="plus" role="img" xmlns="http://www.w3.org/2000/svg"
                             viewBox="0 0 448 512" data-fa-i2svg="">
                            <path fill="currentColor"
                                  d="M416 208H272V64c0-17.67-14.33-32-32-32h-32c-17.67 0-32 14.33-32 32v144H32c-17.67 0-32 14.33-32 32v32c0 17.67 14.33 32 32 32h144v144c0 17.67 14.33 32 32 32h32c17.67 0 32-14.33 32-32V304h144c17.67 0 32-14.33 32-32v-32c0-17.67-14.33-32-32-32z">
                            </path>
                        </svg><!-- <i class="fa fa-plus mr-1"></i> Font Awesome fontawesome.com -->
                        Mark Attendance
                    </button>

                    <div class="modal" id="add-attendance">
                        <form id="MarkAttendance" method="post" asp-action="Index" asp-controller="EmployeeAttendance" class="needs-validation" enctype="multipart/form-data" novalidate>

                            <div class="modal-dialog modal-xl">
                                <div class="modal-content">
                                    <!-- Modal Header -->
                                    <div class="modal-header">
                                        <h4 class="mb-0 p-20 f-21 font-weight-normal text-capitalize">
                                            Mark Attendance
                                        </h4>
                                        <button type="button" class="btn-close"
                                                data-bs-dismiss="modal"></button>
                                    </div>

                                    <!-- Modal body -->
                                    <div class="modal-body">
                                        <div class="add-client bg-white rounded">


                                            <div class="row px-4">

                                                <div class="col-lg-3 col-md-6">
                                                    <label class="f-14 text-dark-grey mb-12 mt-3" data-label="" for="department_id">
                                                        Department

                                                    </label>
                                                    <div class="form-group mb-0">

                                                        <div class="dropdown">
                                                            <select name="DepartmentID" id="DepartmentID" asp-for="DepartmentID" data-live-search="true" class="form-control dropdown-toggle" data-size="8">
                                                                <option value="0">--</option>
                                                                <option value="1">Marketing</option>
                                                                <option value="2">Sales</option>
                                                                <option value="3">Human Resources</option>
                                                                <option value="4">Public Relations</option>
                                                                <option value="5">Research</option>
                                                                <option value="6">Finance</option>
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>

                                                <div class="col-md-9">
                                                    <label class="f-14 text-dark-grey mb-12 mt-3" data-label="" for="department_id">
                                                        Employee

                                                    </label>
                                                    <div class="form-group mb-0">

                                                        <div class="dropdown">
                                                            <select id="employee_id" asp-for="EmployeeId" asp-items="ViewBag.EmployeeList" class="form-control dropdown-toggle">
                                                                <option value="0">--</option>
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>

                                            </div>
                                            <div class="row px-4 pb-4">

                                                <div class="col-lg-3 col-md-6">
                                                    <label class="f-14 text-dark-grey mb-12 mt-3" data-label="" for="location">
                                                        Location

                                                    </label>
                                                    <div class="form-group mb-0">

                                                        <div class="dropdown">
                                                            <select name="location" id="location" data-live-search="true" class="form-control dropdown-toggle" data-size="8">
                                                                <option selected="" value="1">
                                                                    Worksuite
                                                                </option>
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>

                                                <div class="col-lg-3 col-md-6">
                                                    <div class="form-group my-3">
                                                        <label class="f-14 text-dark-grey mb-12" data-label="" for="mark_attendance_by_month">
                                                            Mark Attendance By

                                                        </label>
                                                       <div class="d-flex">
                                                            <div class="form-check-inline custom-control custom-radio mt-2 mr-3">
                                                                <input type="radio" value="month" asp-for="MarkAttendanceBY" class="form-group cursor-pointer" checked="" autocomplete="off">
                                                                <label class=" pb-2 cursor-pointer" for="mark_attendance_by_month">Month</label>
                                                            </div>
                                                            <div class="form-check-inline custom-control custom-radio mt-2 mr-3">
                                                                <input type="radio" value="date" asp-for="MarkAttendanceBY" class="form-group cursor-pointer" autocomplete="off">
                                                                <label class=" pb-2 cursor-pointer" for="mark_attendance_by_dates">Date</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-lg-3 col-md-6 attendance_by_month">
                                                    <label class="f-14 text-dark-grey mb-12 mt-3" data-label="true" for="year">
                                                        Year
                                                        <sup class="f-14 mr-1">*</sup>

                                                    </label>
                                                    <div class="form-group mb-0">

                                                        <div class="dropdown">
                                                            <select name="year" asp-for="Year" id="year" data-live-search="true" class="form-control dropdown-toggle select-picker" data-size="8">
                                                                <option value="">--</option>
                                                                <option selected="" value="2022">2022</option>
                                                                <option value="2021">2021</option>
                                                                <option value="2020">2020</option>
                                                                <option value="2019">2019</option>
                                                                <option value="2018">2018</option>
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>

                                                <div class="col-lg-3 col-md-6 attendance_by_month">
                                                    <label class="f-14 text-dark-grey mb-12 mt-3" data-label="true" for="month">
                                                        Month
                                                        <sup class="f-14 mr-1">*</sup>

                                                    </label>
                                                    <div class="form-group mb-0">

                                                        <div class="dropdown">
                                                            <select name="month" asp-for="Month" id="month" data-live-search="true" class="form-control dropdown-toggle select-picker" data-size="8">
                                                                <option value="">--</option>
                                                                <option value="01">
                                                                    January
                                                                </option>
                                                                <option value="02">
                                                                    February
                                                                </option>
                                                                <option value="03">
                                                                    March
                                                                </option>
                                                                <option value="04">
                                                                    April
                                                                </option>
                                                                <option value="05">
                                                                    May
                                                                </option>
                                                                <option value="06">
                                                                    June
                                                                </option>
                                                                <option value="07">
                                                                    July
                                                                </option>
                                                                <option value="08">
                                                                    August
                                                                </option>
                                                                <option selected="" value="09">
                                                                    September
                                                                </option>
                                                                <option value="10">
                                                                    October
                                                                </option>
                                                                <option value="11">
                                                                    November
                                                                </option>
                                                                <option value="12">
                                                                    December
                                                                </option>
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>

                                                <div class="col-lg-4 col-md-6 d-none multi_date_div">
                                                    <div class="form-group my-3">
                                                        <label class="f-14 text-dark-grey mb-12" data-label="" for="multi_date">
                                                            You can select multiple dates.

                                                        </label>

                                                        <input type="text" class="form-control height-35 f-14" placeholder="You can select multiple dates." value="19-09-2022" name="multi_date" id="multi_date" autocomplete="off">

                                                    </div>
                                                </div>

                                            </div>
                                            <div class="row px-4">
                                                <div class="col-lg-4 col-md-6 col-xl-3">
                                                    <div class="bootstrap-timepicker timepicker">
                                                        <div class="form-group my-3">
                                                            <label class="f-14 text-dark-grey mb-12" data-label="true" for="start_time">
                                                                Clock In
                                                                <sup class="f-14 mr-1">*</sup>

                                                            </label>

                                                            <input type="text" asp-for="CheckInTime" class="form-control height-35 f-14" placeholder="e.g. 10:00 AM" value="09:00 am" autocomplete="off">

                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-lg-4 col-md-6 col-xl-3">
                                                    <div class="bootstrap-timepicker timepicker">
                                                        <div class="form-group my-3">
                                                            <label class="f-14 text-dark-grey mb-12" data-label="" for="end_time">
                                                                Clock Out

                                                            </label>

                                                            <input type="text" asp-for="CheckOutTime" class="form-control height-35 f-14" placeholder="e.g. 10:00 AM" value="06:00 pm" autocomplete="off">

                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-lg-4 col-md-6 col-xl-3">
                                                    <div class="form-group my-3">
                                                        <label class="f-14 text-dark-grey mb-12" data-label="" for="late_yes">
                                                            Late

                                                        </label>
                                                        <div class="d-flex">
                                                            <div class="form-check-inline custom-control custom-radio mt-2 mr-3">
                                                                <input type="radio" value="yes" asp-for="IsLate" class="form-group cursor-pointer" autocomplete="off">
                                                                <label class=" pb-2 cursor-pointer" for="late_yes">Yes</label>
                                                            </div>
                                                            <div class="form-check-inline custom-control custom-radio mt-2 mr-3">
                                                                <input type="radio" value="no" asp-for="IsLate" class="form-group cursor-pointer" autocomplete="off">
                                                                <label class=" pb-2 cursor-pointer" for="late_no">No</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-lg-4 col-md-6 col-xl-3">
                                                    <div class="form-group my-3">
                                                        <label class="f-14 text-dark-grey mb-12" data-label="" for="half_day_yes">
                                                            Half Day

                                                        </label>

                                                        <div class="d-flex">
                                                            <div class="form-check-inline custom-control custom-radio mt-2 mr-3">
                                                                <input type="radio" asp-for="IsHalfDay" value="yes" class="form-group cursor-pointer" autocomplete="off">
                                                                <label class=" pb-2 cursor-pointer" for="half_day_yes">Yes</label>
                                                            </div>
                                                            <div class="form-check-inline custom-control custom-radio mt-2 mr-3">
                                                                <input type="radio" asp-for="IsHalfDay" value="no" class="form-group cursor-pointer" autocomplete="off">
                                                                <label class=" pb-2 cursor-pointer" for="half_day_no">No</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>


                                            </div>

                                            <div class="row px-4">
                                                <div class="col-lg-3 col-md-3">
                                                    <label class="f-14 text-dark-grey mb-12 mt-3" data-label="true" for="work_from_type">
                                                        Working From
                                                        <sup class="f-14 mr-1">*</sup>

                                                    </label>
                                                    <div class="form-group mb-0">

                                                        <div class="dropdown">
                                                      @*      <select name="year" asp-for="Year" id="year" data-live-search="true" class="form-control dropdown-toggle select-picker" data-size="8">
                                                                <option value="">--</option>
                                                                <option selected="" value="2022">2022</option>
                                                                <option value="2021">2021</option>
                                                                <option value="2020">2020</option>
                                                                <option value="2019">2019</option>
                                                                <option value="2018">2018</option>
                                                            </select>*@

                                                      <select name="WorkingFrom" id="WorkingFrom" asp-for="WorkingFrom" data-live-search="true" class="form-control dropdown-toggle select-picker" data-size="8">
                                                                <option value="">--</option>
                                                                <option value="Office">Office</option>
                                                                <option value="Home">Home</option>
                                                                <option value="Other">Other</option>
                                                            </select>
                                                        </div>

                                                    </div>
                                                </div>

                                                <div class="col-lg-3 col-md-6" id="other_place" style="display:none">
                                                    <div class="form-group my-3">
                                                        <label class="f-14 text-dark-grey mb-12" data-label="true" for="working_from">
                                                            Other Locations
                                                            <sup class="f-14 mr-1">*</sup>

                                                        </label>

                                                        <input type="text" class="form-control height-35 f-14" placeholder="" value="" name="working_from" id="working_from" autocomplete="off">

                                                    </div>
                                                </div>

                                            </div>


                                        </div>
                                    </div>

                                    <!-- Modal footer -->
                                    <div class="modal-footer justify-content-start px-4">
                                        <button type="button" class="btn btn-outline-danger"
                                                data-bs-dismiss="modal">
                                            Close
                                        </button>
                                        <button type="submit" class="btn btn-primary">Save</button>
                                    </div>

                                </div>
                            </div>

                        </form>
                    </div>

                    <div class="px-3">
                        <button type="button" class="btn btn-outline-secondary rounded f-14 p-2 mr-3 mb-2 mb-lg-0 bg-white"
                                id="export-all">
                            <svg class="svg-inline--fa fa-file-export fa-w-18 mr-1" aria-hidden="true" focusable="false"
                                 data-prefix="fa" data-icon="file-export" role="img" xmlns="http://www.w3.org/2000/svg"
                                 viewBox="0 0 576 512" data-fa-i2svg="">
                                <path fill="currentColor"
                                      d="M384 121.9c0-6.3-2.5-12.4-7-16.9L279.1 7c-4.5-4.5-10.6-7-17-7H256v128h128zM571 308l-95.7-96.4c-10.1-10.1-27.4-3-27.4 11.3V288h-64v64h64v65.2c0 14.3 17.3 21.4 27.4 11.3L571 332c6.6-6.6 6.6-17.4 0-24zm-379 28v-32c0-8.8 7.2-16 16-16h176V160H248c-13.2 0-24-10.8-24-24V0H24C10.7 0 0 10.7 0 24v464c0 13.3 10.7 24 24 24h336c13.3 0 24-10.7 24-24V352H208c-8.8 0-16-7.2-16-16z">
                                </path>
                            </svg><!-- <i class="fa fa-file-export mr-1"></i> Font Awesome fontawesome.com -->
                            Export
                        </button>
                    </div>
                    <a href="#"
                       class="btn btn-outline-secondary rounded f-14 p-2 mr-3 openRightModal float-left bg-white">
                        <svg class="svg-inline--fa fa-file-upload fa-w-12 mr-1" aria-hidden="true" focusable="false"
                             data-prefix="fa" data-icon="file-upload" role="img" xmlns="http://www.w3.org/2000/svg"
                             viewBox="0 0 384 512" data-fa-i2svg="">
                            <path fill="currentColor"
                                  d="M224 136V0H24C10.7 0 0 10.7 0 24v464c0 13.3 10.7 24 24 24h336c13.3 0 24-10.7 24-24V160H248c-13.2 0-24-10.8-24-24zm65.18 216.01H224v80c0 8.84-7.16 16-16 16h-32c-8.84 0-16-7.16-16-16v-80H94.82c-14.28 0-21.41-17.29-11.27-27.36l96.42-95.7c6.65-6.61 17.39-6.61 24.04 0l96.42 95.7c10.15 10.07 3.03 27.36-11.25 27.36zM377 105L279.1 7c-4.5-4.5-10.6-7-17-7H256v128h128v-6.1c0-6.3-2.5-12.4-7-16.9z">
                            </path>
                        </svg><!-- <i class="fa fa-file-upload mr-1"></i> Font Awesome fontawesome.com -->
                        Import
                    </a>
                </div>

                <div class="shadow attendance-table p-3 bg-white">
                    <div class="row py-3">
                        <div class="col-md-12">
                            <span class="f-w-500 mr-1">Note:</span>
                            <svg class="svg-inline--fa fa-star fa-w-18 text-warning" aria-hidden="true"
                                 focusable="false" data-prefix="fa" data-icon="star" role="img"
